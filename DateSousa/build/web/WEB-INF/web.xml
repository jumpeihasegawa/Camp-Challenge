    <servlet-mapping>
        <servlet-name>Kadai4Servlet</servlet-name>
        <url-pattern>/Kadai4Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Ouyou5Servlet</servlet-name>
